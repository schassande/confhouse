                      "type": "string",
                      "pattern": "#[0-9a-fA-F]{3}(?:[0-9a-fA-F]{3})?"
                    },
                    "secondaryContrastHex": {
                      "type": "string",
                      "pattern": "#[0-9a-fA-F]{3}(?:[0-9a-fA-F]{3})?"
                    },
                    "tertiaryHex": {
                      "type": "string",
                      "pattern": "#[0-9a-fA-F]{3}(?:[0-9a-fA-F]{3})?"
                    },
                    "tertiaryContrastHex": {
                      "type": "string",
                      "pattern": "#[0-9a-fA-F]{3}(?:[0-9a-fA-F]{3})?"
                    }
                  },
                  "required": [
                    "primaryHex",
                    "primaryContrastHex",
                    "secondaryHex",
                    "secondaryContrastHex",
                    "tertiaryHex",
