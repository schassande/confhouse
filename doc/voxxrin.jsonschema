                    "primaryContrastHex": {
                      "type": "string",
                      "pattern": "#[0-9a-fA-F]{3}(?:[0-9a-fA-F]{3})?"
                    },
                    "secondaryHex": {
                      "type": "string",
                      "pattern": "#[0-9a-fA-F]{3}(?:[0-9a-fA-F]{3})?"
                    },
                    "secondaryContrastHex": {
                      "type": "string",
                      "pattern": "#[0-9a-fA-F]{3}(?:[0-9a-fA-F]{3})?"
                    },
                    "tertiaryHex": {
                      "type": "string",
                      "pattern": "#[0-9a-fA-F]{3}(?:[0-9a-fA-F]{3})?"
